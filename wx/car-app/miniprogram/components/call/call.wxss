/* components/call/call.wxss */
.callBox{
  height: 128rpx;
  width: 128rpx;
  border-radius: 50%;
}
.icon{
  width: 100%;
  height: 100%;
}
.icon image{
  width:100%;
  height:100%;
}
.backgournd{
  position:fixed;
  top:0;
  bottom:0;
  right:0;
  left:0;
  background:rgb(0,0,0,0.6);
  display:flex;
  align-items:flex-end;
  align-content:center;
  z-index: 99999
}
.binner{
   text-align:center;
   width:100%;
   height:40vw;
   background:#FFFEFF;
   border-radius:10px 10px 0 0;
   display:flex;
   flex-direction:column;
   justify-content:space-between;
}
.bitem{
  height:13vw;
  line-height: 11vw;
  border-bottom: 1rpx solid #dbdbdb;
}
.hoverClass{
  background: #dbdbdb
}
