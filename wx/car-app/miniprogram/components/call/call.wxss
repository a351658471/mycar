.callBox{
  position: fixed;
  right:20rpx;
  bottom: 20rpx;
  height: 2.5rem;
  width: 2.5rem;
  border: 1rpx solid #515151;
  border-radius: 50%;
  background: #fff
}
.icon{
  width: 85%;
  height: 85%;
  margin: auto;
  transform: translateY(10%)
}
.icon image{
  width:100%;
  height:100%;
  
}
.backgournd{
    position:fixed;
    top:0;
    bottom:0;
    right:0;
    left:0;
    background-color:#333333d1;
    display:flex;
    align-items:flex-end;
    align-content:center;
}
.binner{
   text-align:center;
   width:100%;
   height:40vw;
   background:#FFFEFF;
   border-radius:10px 10px 0 0;
   display:flex;
   flex-direction:column;
   justify-content:space-between;
}
.bitem{
  height:13vw;
  line-height: 11vw;
  border-bottom: 1rpx solid #dbdbdb;
}
.hoverClass{
  background: #dbdbdb
}


