.enterBlock{
  width: 100%;
  height: 100%;
}
.container1 {
  margin-top: .5rem;
  box-sizing: border-box;
  background: #fff;
  width: 90%;
  height:125px;
  padding: 10px;
  margin: auto;
  overflow-y: hidden;
  border: 2rpx solid #E1E1E1;
  border-bottom: none;
  border-radius: 6rpx 6rpx 0 0;
}
.imgContent{
  width: 100%;
  height: 100%;
  position: relative;
  border: 1px solid #F5F5F5
}
.toolbar {
  width: 90%;
  margin: auto;
  border: 2rpx solid #E7E7E7;
  border-radius: 0 0 6rpx 6rpx;
  box-sizing: border-box;
  padding: 10rpx 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: linear-gradient(to bottom,#FFFFFF,#F8F8F8);
}
.toAnywhere{
  display: flex;
  align-items: center;
}
.iconItem{
  margin-right: 3vw;
  width: 78rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  border: 1rpx solid #E1E1E1;
  border-radius: 6rpx;
}
.toolbarDelete{
  font-size: 24rpx;
  color: #999999
}
.bAdd{
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin:76rpx auto; 
  width: 168rpx;
  height: 76rpx;
  text-align: center;
  background: #3377FE;
  border: 1px solid #2478D5;
  border-radius: 6rpx;
}
.bAdd text{
  font-size: 32rpx;
  color: #FFFFFF;
  padding-left: 16rpx;
}
.bAdd image{
  width: 32rpx;
  height: 32rpx;
}
.buttonBox{
  width: 80%;
  display: flex;
  margin: 3rem auto;
  border-radius: 6rpx;
  background: #3377FE;
  border: 1px solid #175DD2;
}
.bItem{
  width: 33%;
  text-align: center;
  color: white;
  padding: 16rpx 0;
  font-size: 32rpx;
  
}