page {
  background: #f5f5f5;
  height: 100%;
}

.addCar {
  height: 100%;
}

.scrollBox {
  height: 100%;
  -webkit-overflow-scrolling: touch;
}

.baseTitle, .labelTitle, .labelAdd {
  padding: 28rpx 24rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.labelAdd {
  font-size: 0.8rem;
  color: #FE2A45;
}

.baseContent {
  color: #7b7575;
  background: #fffeff;
}

.borderBottom {
  width: 100%;
  border-bottom: 1px solid #f8f6f7;
}

.baseItem {
  display: flex;
  height: 13vw;
  line-height: 13vw;
  padding: 0 3vw;
}

.inputItem {
  display: flex;
  align-items: center;
  margin-left: 10vw;
  font-size: 28rpx;
}

.radioItem {
  margin-left: 10vw;
  font-size: 28rpx;
}

.ItemName {
  width: 20vw;
  text-align: right;
  color: #7b7575;
  font-size: 30rpx;
  font-weight: bold;
}

.radio {
  margin-right: 5vw;
}

radio .wx-radio-input {
  width: 3vw;
  height: 3vw;
}

radio .wx-radio-input.wx-radio-input-checked {
  background: #FE2A45;
  border: 1px solid #a3a1a2;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  content: '';
}

.labelItem {
  display: flex;
  align-items: center;
  background: #fffeff;
  font-size: 20rpx;
  padding: 25rpx 44rpx 40rpx 44rpx;
  color: #FE2A45;
  flex-wrap: wrap;
}

.labelText {
  border: 2rpx solid #FE2A45;
  padding: 0 8rpx;
  border-radius: 6rpx;
  text-align: center;
  font-size: 20rpx;
  margin-top: 15rpx;
}

.deleteIcon {
  height: 1rem;
  width: 1rem;
  margin-left: 0.3rem;
  border-radius: 50%;
  border: 1px solid #fe2a45;
  position: relative;
  z-index: 1;
}

.deleteIcon::after {
  content: '';
  width: 0.5rem;
  height: 1px;
  background: #fe2a45;
  position: absolute;
  z-index: 1;
  top: 50%;
  left: 25%;
}

.labelHeader {
  display: flex;
  justify-content: space-between;
}

.imgBox {
  width: 100%;
}

.addImgBox {
  height: 25vw;
  display: flex;
  width: 100%;
  background: #fff;
  align-items: center;
}

.imgItem {
  width: 30%;
  height: 80%;
  text-align: center;
  position: relative;
  margin-right: 5vw;
  z-index: 1;
}

.itemImg {
  width: 100%;
  height: 100%;
  border: 1px solid #9a979b;
  border-radius: 5px;
}

.deleteImg {
  width: 15%;
  position: absolute;
  z-index: 1;
  right: 0;
  top: 0;
}

.deleteImg image {
  width: 100%;
  background: #fff;
}

.itemAdd {
  width: 25%;
  height: 80%;
  border: 1px dashed #b5b2b5;
  text-align: center;
  font-size: 14px;
  color: #b5b2b5;
}

.itemAdd image {
  width: 60%;
}

.next {
  width: 90%;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  background: linear-gradient(to right, #fd7493, #fe2a45);
  color: #fff;
  border-radius: 16rpx;
  margin: 20rpx auto;
  position: fixed;
  bottom: 0;
  z-index: 9999;
  margin-left: 5%;
  font-size: 36rpx;
  letter-spacing: 1rpx;
}

.labelHeader {
  display: flex;
  justify-content: space-between;
}

.detailSave {
  margin-right: 3vw;
  display: flex;
  width: 40%;
  justify-content: space-between;
  align-items: center;
}

.rightItem {
  height: 7vw;
  line-height: 7vw;
  background: #FE2A45;
  color: #fff;
  width: 30%;
  border-radius: 5px;
  font-size: 13px;
  text-align: center;
}

.container1 {
  background: #fff;
  padding: 44rpx 22rpx;
}

.container1 textarea {
  margin: 10rpx 0 80rpx 0;
}

.toolbar {
  box-sizing: border-box;
  padding: 0 20rpx 200rpx 20rpx;
  height: 50px;
  width: 100%;
  display: flex;
  align-items: center;
  border-left: none;
  border-right: none;
  background: #fff;
}

.iconfont {
  font-size: 20px !important;
}

.iconItem {
  margin-right: 3vw;
}

.imgContent {
  width: 100%;
  position: relative;
  z-index: 1;
  border: 1px solid #f5f5f5;
}

.videoDelete {
  background: #fff;
  width: 6vw;
  height: 6vw;
  position: absolute;
  z-index: 1;
  top: 0;
  right: 0;
}

.videoDelete image {
  width: 100%;
}

