<!--miniprogram/pages/carManage/carManage.wxml-->
<view class="carManage">
  
  <!-- tab -->
  <mg-tab tabList="{{tabList}}" bind:tabClick="tabClick" fontSize="17px">
    <view slot="rightBox" class="rightBox" bindtap="jumpToAddcar" style="font-size:14px">添加新车</view>
  </mg-tab>

   <!-- 车列表 -->
  <my-carslist items="{{items}}" carData="{{carData}}" bind:caritemClick="caritemClick" bind:saleGoods="saleGoods" bind:soldGoods="soldGoods" bind:lowGoods="lowGoods" bind:deleteGoods="deleteGoods" bind:loadMore="loadMore" isLoading="{{isLoading}}" noMore="{{noMore}}" bind:toEdit="toEdit"></my-carslist>
</view>