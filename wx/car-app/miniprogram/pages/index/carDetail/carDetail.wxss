/* pages/home/home.wxss */
page{

  height:100%
}
.active{
  color: red
}
.outbox{
  height: 100%
}
.carDetail{
  padding: 1rem 0;
  border-top: 5vw solid #F7F7F7;
  width:100%;
  height:17vw;
  background-color: #FFFFFF;
}
.heard-name, .heard-right, detail-body{
  font-size: 12px
}
.detail-heard{
  width: 90%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between
}
.detail-body{
  width: 90%;
  margin: 46rpx auto 0 auto;
  display: flex;
  justify-content: space-between
}
.body-left{
  display: flex;
  justify-content: space-between
}
.left-item{
  font-size: 12px;
  border: 1px solid #FE2A45;
  border-radius: 5px;
  padding: 3px 10px;
  color: #FE2A45;
  margin-right: 0.5rem
}

.bottomBox{
  /* position: absolute;
  bottom: 0;
  top: 50%;
  left: 0;
  right: 0; */
  border-top: 5vw solid #FFFFFF;
  padding-top:1vw; 
  padding-bottom:10rem ;
}

.backgournd{
    position:fixed;
    top:0;
    bottom:0;
    right:0;
    left:0;
    background-color:#333333d1;
    display:flex;
    align-items:flex-end;
    align-content:center;
}
.binner{
   text-align:center;
   width:100%;
   height:40vw;
   background:#FFFFFF;
   border-radius:10px 10px 0 0;
   display:flex;
   flex-direction:column;
   justify-content:space-between;
}
.bitem{
  height:13vw;
  line-height: 11vw
}
.hoverClass{
  background: #dbdbdb
}
.callIcon{
  position: fixed;
  right:5%;
  bottom: 5%;
}


