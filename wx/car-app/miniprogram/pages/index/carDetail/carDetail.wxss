/* pages/home/home.wxss */
page{

  height:100%
}
.active{
  color: red
}
.outbox{
  height: 100%
}
.carDetail{
  padding: 1rem 0;
  border-top: 5vw solid #F7F7F7;
  width:100%;
  height:17vw;
  background-color: #FFFFFF;
}
.heard-name, detail-body{
  font-size: 28rpx;
}
.heard-right{
  display: flex;
  align-items: center;
  font-size: 28rpx;
  height:5vw
}
.share{
  width: 20px;
  height: 20px;
  margin: auto;
  margin-left:2vw
}
.detail-heard{
  width: 90%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between
}
.detail-body{
  width: 90%;
  margin: 46rpx auto 0 auto;
  display: flex;
  justify-content: space-between
}
.body-left{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.left-item{
  font-size: 24rpx;
  border: 2rpx solid #FE2A45;
  border-radius: 6rpx;
  padding: 0 20rpx;
  color: #FE2A45;
  margin-right: 30rpx;
}

.bottomBox{
  border-top: 5vw solid #FFFFFF;
  padding-top:1vw; 
  background: #FFFFFF;
  margin-top: 28rpx;
}

.backgournd{
    position:fixed;
    top:0;
    bottom:0;
    right:0;
    left:0;
    background-color:#333333d1;
    display:flex;
    align-items:flex-end;
    align-content:center;
}
.binner{
   text-align:center;
   width:100%;
   height:40vw;
   background:#FFFFFF;
   border-radius:10px 10px 0 0;
   display:flex;
   flex-direction:column;
   justify-content:space-between;
}
.bitem{
  height:13vw;
  line-height: 11vw
}
.hoverClass{
  background: #dbdbdb
}
.callIcon{
  position: fixed;
  right:5%;
  bottom: 5%;
}
.boxBox{
  height: 20%;
  width: 100%;
  background: #fff
}


/* 分享 */
.shareBox{
  background: #333333d1;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0
}
.boxBottom{
  position: absolute;
  bottom: 0;
  left: 0;
  right:0;
  height:30vh;
  background: #fff;
  display: flex;
  flex-direction:column;
}
.title{
  height:10vw;
  line-height: 10vw;
  text-align: center;
  font-size: 35rpx;
}
.shareContent{
  display: flex;
  justify-content: space-around;
  align-items: center;
  height:18vh
}
.shareImg{
  width:13vw;
  margin: 0 auto
}
.shareText{
  font-size: 13px
}
.cancel{
  font-size: 35rpx;
  color: #FE2A45;
  height:12vw;
  line-height: 12vw;
  text-align: center;
  border-top: 1px solid #dbdbdb
}
/* 画布区域 */
.cardBox{
  position: absolute;
  width: 100%;
  height:100%;
  background-image: linear-gradient(to bottom,#FFD146,#FE454D)

}
.canvasBox{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  border-radius: 10px
}

.downCard{
  width: 60%;
  height:13vw;
  border-radius: 30px;
  background:#FFE226;
  line-height: 13vw;
  text-align: center;
  margin: 5vw auto
}






