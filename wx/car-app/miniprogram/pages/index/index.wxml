<view class='outBox'>
  <scroll-view class='crollView' scroll-y style="height:100%">

    <!-- 轮播图 -->
    <view style="margin-top:36rpx;">
      <my-swiper imgList="{{shop.swipers}}" isDots="{{true}}" bind:previewImage="previewImage"></my-swiper>
    </view>
    <!-- tab -->
    <view style="margin:68rpx auto 30rpx 46rpx;">
        <my-ltab bind:tabClick="tabClick"></my-ltab>
    </view>

    <!-- 车列表 -->
    <my-carslist carData="{{resData}}" bind:caritemClick="caritemClick" bind:loadMore="loadMore" isLoading="{{isLoading}}" noMore="{{noMore}}"></my-carslist>
  </scroll-view>

</view>
<my-call class="callIcon" bind:call="call"></my-call>