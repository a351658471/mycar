.labelHeader{
  margin:.5rem auto;
  width: 90%;
  display: flex;
  justify-content: space-between;
}
.labelTitle,.labelAdd{
  font-size: 1rem;
}
.detailSave{
  display: flex;
  width: 40%;
  justify-content: space-around;
  align-items: center
}
.rightItem{
  height:7vw;
  line-height: 7vw;
  background: #3377FE;
  color: #fff;
  width: 15vw!important; 
  border-radius: 5px;
  font-size: 13px;
  text-align: center
}
.container1 {
  margin-top: .5rem;
  box-sizing: border-box;
  background: #fff;
  width: 90%;
  height:25vh;
  padding: 10px;
  margin: auto;
  overflow-y: hidden
}
input::placeholder{
  color: #E1E1E1;
}
.toolbar {
  width: 90%;
  margin: auto;
  border: 1px solid #E7E7E7;
  box-sizing: border-box;
  padding: 10rpx 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* justify-content: space-between; */
  /* border: 1px solid #ECECEC; */
  border-left: none;
  border-right: none;
  background: linear-gradient(to right,#FFFFFF,#F8F8F8);
}
.toAnywhere{
  display: flex;
  align-items: center;
}
.toolbarDelete{
  font-size: 24rpx;
  color: #999999
}
.iconfont{
  font-size: 20px !important
}
.iconItem{
  margin-right: 3vw;
  width: 78rpx;
  height: 42rpx;
  text-align: center;
  border: 1rpx solid #E1E1E1;
  border-radius: 6rpx;
}
.iconItem image{
  width: 34rpx;
  height: 20rpx;
}
.imgContent{
  width: 100%;
  height: 100%;
  position: relative;
  border: 1px solid #F5F5F5
}

.videoDelete{
  background: #fff;
  width: 6vw;
  height: 6vw;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1
}
.videoDelete image{
  width: 100%
}
.bAdd{
  display: flex;
  align-items: center;
  justify-content: center;
  margin:76rpx auto; 
  width: 168rpx;
  text-align: center;
  background: #3377FE;
  border: 1px solid #2478D5;
  border-radius: 6rpx;
  padding: 18rpx 0;
}
.bAdd image{
  width: 32rpx;
  height: 32rpx;
}
.bAdd text{
  font-size: 32rpx;
  color: #FFFFFF;
  padding-left: 16rpx;
}
.buttonBox{
  width: 80%;
  display: flex;
  margin: 1.5rem auto;
  border-radius: 6rpx;
  background: #3377FE;
  border: 1px solid #175DD2;
}
.bItem{
  width: 33%;
  text-align: center;
  color: white;
  padding: 16rpx 0;
  font-size: 32rpx;
  
}
