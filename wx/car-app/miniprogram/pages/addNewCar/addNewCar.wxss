page{
  background: #F5F5F5;
  height: 100%;
}
.addCar{
  height: 100%
}
.baseTitle,.labelTitle,.labelAdd{
  padding: 28rpx 24rpx;
  font-size: 32rpx;
  font-weight: bold;
}
.labelAdd{
  font-size: 28rpx;
  color:#3377FE
}
.baseContent{
  color: #7B7575;
  background: #FFFEFF;

}
.borderBottom{
  width: 100%;
  border-bottom: 1px solid #F8F6F7
}
.baseItem{
  display: flex;
  height: 13vw;
  line-height: 13vw;
  padding:0 3vw;
}
.inputItem{
  display: flex;
  align-items: center;
  margin-left: 10vw;
  font-size: 28rpx;
}
.picker{
  width:50vw
}
.radioItem{
   margin-left: 10vw;
   font-size: 28rpx;
}
.ItemName{
  width:20vw;
  text-align: right;
  font-size: 30rpx;
  color: #7B7575;
  font-weight: bold;
}
.radio{
  margin-right: 40rpx;
}
radio .wx-radio-input{
  width: 3vw;
  height: 3vw
}
radio .wx-radio-input.wx-radio-input-checked{
  background:#3377FE !important;
  border: 1px solid #A3A1A2 !important
}
radio .wx-radio-input.wx-radio-input-checked::before{
  content: '';
  
}
.labelItem{
  display: flex;
  align-items: center;
  background: #FFFEFF;
  font-size: 24rpx;
  padding: 25rpx 44rpx 40rpx 44rpx;
  color: #6C6C6C;
  flex-wrap: wrap;
}
.labelText{
  background:#F8F8FA;
  padding: 2rpx 14rpx;
  border-radius: 6rpx;
  text-align: center;
  font-size: 24rpx;
  margin-top: 15rpx; 
}
.deleteIcon image{
  width: 28rpx;
  height: 28rpx;
}
.labelHeader{
  display: flex;
  justify-content: space-between;
}
.imgBox{
  width: 100%
}
.addImgBox{
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  background: #fff;
  align-items: center;
  padding: 20rpx 0 0 0;
}
.imgItem{
  width:30%;
  height:20vw;
  position: relative;
  margin: 0 20rpx 20rpx 0;
}
.itemImg{
  width: 100%;
  height: 100%;
  border: 2rpx solid #979797;
  margin-left: 10rpx;
}
.deleteImg{
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  right:-12rpx;
  top: -5rpx;
}
.deleteImg image{
  width: 100%;
  height: 100%;
  background: #fff
}
.itemAdd{
  width: 30%;
  height:22.5vw;
  margin: 0 0 0 10rpx;
}
.itemAdd image{
  width:100%
}
.next{
  width: 90%;
  height:12vw;
  text-align: center;
  line-height: 12vw;
  background: linear-gradient(to right,#3377FE,#3377FE);
  color: #fff;
  border-radius: 10px;
  margin: 5vw auto
}

.labelHeader{
  display: flex;
  justify-content: space-between;
}
.detailSave{
  margin-right: 3vw;
  display: flex;
  width: 40%;
  justify-content: space-between;
  align-items: center
}
.rightItem{
  height:7vw;
  line-height: 7vw;
  background: #FE2A46;
  color: #fff;
  width: 30%;
  border-radius: 5px;
  font-size: 13px;
  text-align: center
}
.container1 {
  background: #fff;
  padding: 0 3vw
}
.toolbar {
  box-sizing: border-box;
  padding: 0 10px;
  height: 50px;
  width: 100%;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  /* border: 1px solid #ECECEC; */
  border-left: none;
  border-right: none;
  background: #fff
}
.iconfont{
  font-size: 20px !important
}
.iconItem{
  margin-right: 3vw
}
.imgContent{
  width: 90%;
  text-align: center;
  position: relative;
  border: 1px solid #F5F5F5
}
videoContent{
  width: 100%;
  position: relative;
  border: 1px solid #F5F5F5
}
.videoDelete{
  background: #fff;
  width: 6vw;
  height: 6vw;
  position: absolute;

  right: 0
}
.videoDelete image{
  width: 100%
}

